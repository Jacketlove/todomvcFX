<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<HBox styleClass="add_item_root" fx:controller="todomvcfx.fluxfx.views.additems.AddItemsView"
      alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <CheckBox fx:id="selectAll" mnemonicParsing="false"/>
    <TextField fx:id="addInput" promptText="What needs to be done?" HBox.hgrow="ALWAYS"/>
    <stylesheets>
        <URL value="@additems.css"/>
    </stylesheets>
</HBox>
